<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BaordMapper">
  <select id="selectBoard" resultType="org.comstudy21.myapp.board.model.BoardDTO">
    select * from board_ex where seq = #{seq}
  </select>
  
  <select id="selectAll"  resultType="org.comstudy21.myapp.board.model.BoardDTO">
     select * from board_ex;
  </select>
</mapper>